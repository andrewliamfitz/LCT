Cell["Cosine Basis", "Subsection",
 CellChangeTimes->{{3.796133973485923*^9, 
  3.796133975506131*^9}},ExpressionUUID->"658bef35-761b-4f17-9272-\
24ff6f2e57dd"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"LargeNCos", "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"m", "\[Equal]", "n"}], ",", 
      RowBox[{"4", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], "n"], "-", "EulerGamma", "+", 
         RowBox[{"2", " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{"n", " ", "\[Pi]"}], "]"}]}], "-", 
         RowBox[{"CosIntegral", "[", 
          RowBox[{"2", " ", "n", " ", "\[Pi]"}], "]"}], "+", 
         RowBox[{"Log", "[", 
          FractionBox["2", 
           RowBox[{"n", " ", "\[Pi]"}]], "]"}], "+", 
         RowBox[{"n", " ", "\[Pi]", " ", 
          RowBox[{"SinIntegral", "[", 
           RowBox[{"n", " ", "\[Pi]"}], "]"}]}]}], ")"}]}], ",", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", "-", "n"}], ")"}], " ", 
         RowBox[{"(", 
          RowBox[{"m", "+", "n"}], ")"}]}]], "2", " ", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], 
          RowBox[{"m", "+", "n"}]]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", 
          SuperscriptBox["m", "2"], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{"m", " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["m", "2"]}], "+", 
            SuperscriptBox["n", "2"]}], ")"}], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"m", "-", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{
          SuperscriptBox["n", "2"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "2"}], " ", 
             RowBox[{"CosIntegral", "[", 
              RowBox[{"n", " ", "\[Pi]"}], "]"}]}], "+", 
            RowBox[{"CosIntegral", "[", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"m", "+", "n"}], ")"}], " ", "\[Pi]"}], "]"}], "-", 
            RowBox[{"Log", "[", 
             RowBox[{
              RowBox[{"-", "1"}], "+", 
              FractionBox[
               SuperscriptBox["m", "2"], 
               SuperscriptBox["n", "2"]]}], "]"}]}], ")"}]}], "+", 
         RowBox[{
          SuperscriptBox["m", "2"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"CosIntegral", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"m", "+", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
            
            RowBox[{"Log", "[", 
             RowBox[{"1", "-", 
              FractionBox[
               SuperscriptBox["n", "2"], 
               SuperscriptBox["m", "2"]]}], "]"}]}], ")"}]}]}], ")"}]}]}], 
     "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LargeNCos", "[", 
    RowBox[{"0", ",", "n_"}], "]"}], ":=", "0"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LargeNCos", "[", 
    RowBox[{"n_", ",", "0"}], "]"}], ":=", "0"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LargeNCos", "[", 
    RowBox[{"0", ",", "0"}], "]"}], ":=", "0"}], ";"}]}], "Input",
 CellChangeTimes->{{3.7961341839656363`*^9, 3.796134191971932*^9}, {
  3.796134382814686*^9, 3.796134407213441*^9}},
