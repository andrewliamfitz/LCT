Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"LargeNSine", "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"m", "\[Equal]", "n"}], ",", 
      RowBox[{"4", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], "n"], "-", "EulerGamma", "+", 
         RowBox[{"CosIntegral", "[", 
          RowBox[{"2", " ", "n", " ", "\[Pi]"}], "]"}], "-", 
         RowBox[{"Log", "[", 
          RowBox[{"2", " ", "n", " ", "\[Pi]"}], "]"}], "+", 
         RowBox[{"n", " ", "\[Pi]", " ", 
          RowBox[{"SinIntegral", "[", 
           RowBox[{"n", " ", "\[Pi]"}], "]"}]}]}], ")"}]}], ",", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"m", "-", "n"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{"m", "+", "n"}], ")"}]}]]}], "2", " ", 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], 
          RowBox[{"m", "+", "n"}]]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], " ", "m", " ", "n", " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{"m", " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"m", "-", "n"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{"m", "+", "n"}], ")"}], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"m", "-", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{"2", " ", "m", " ", "n", " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{"n", " ", "\[Pi]"}], "]"}]}], "-", 
         RowBox[{
          SuperscriptBox["m", "2"], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"m", "+", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{
          SuperscriptBox["n", "2"], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"m", "+", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{"2", " ", "m", " ", "n", " ", 
          RowBox[{"Log", "[", "m", "]"}]}], "-", 
         RowBox[{
          SuperscriptBox["m", "2"], " ", 
          RowBox[{"Log", "[", 
           RowBox[{"m", "-", "n"}], "]"}]}], "+", 
         RowBox[{
          SuperscriptBox["n", "2"], " ", 
          RowBox[{"Log", "[", 
           RowBox[{"m", "-", "n"}], "]"}]}], "-", 
         RowBox[{"2", " ", "m", " ", "n", " ", 
          RowBox[{"Log", "[", "n", "]"}]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"m", "-", "n"}], ")"}], " ", 
          RowBox[{"(", 
           RowBox[{"m", "+", "n"}], ")"}], " ", 
          RowBox[{"Log", "[", 
           RowBox[{"m", "+", "n"}], "]"}]}]}], ")"}]}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
