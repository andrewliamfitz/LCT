           First[FirstPosition[mixonMassCode`Private`diff, 1]]; 
          mixonMassCode`Private`i = Total[Take[mixonMassCode`Private`b, 
             mixonMassCode`Private`ki]]; mixonMassCode`Private`kpj = 
           First[FirstPosition[mixonMassCode`Private`diff, -1]]; 
          mixonMassCode`Private`j = Total[Take[mixonMassCode`Private`bp, 
