Cell["LCT Basis", "Subsection",
 CellChangeTimes->{{3.7961339702618237`*^9, 
  3.796133971739409*^9}},ExpressionUUID->"1d0e7e69-4266-49f3-8c73-\
dd4147523cac"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LargeNLCT", "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"EvenQ", "[", 
      RowBox[{"m", "+", "n"}], "]"}], ",", 
     RowBox[{
      RowBox[{"2", " ", 
       SqrtBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "m"}], "+", "1"}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", "n"}], "+", "1"}], ")"}]}]], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"HarmonicNumber", "[", 
          RowBox[{
           FractionBox["1", "2"], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", "kM"}], ")"}]}], "]"}], "+", 
         RowBox[{"HarmonicNumber", "[", 
          FractionBox["kM", "2"], "]"}], "-", 
         RowBox[{"HarmonicNumber", "[", 
          RowBox[{
           FractionBox["1", "2"], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "-", "km", "+", "kM"}], ")"}]}], "]"}], "-", 
         
         RowBox[{"HarmonicNumber", "[", 
          FractionBox[
           RowBox[{"km", "+", "kM"}], "2"], "]"}]}], ")"}]}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"km", "\[Rule]", 
         RowBox[{"Min", "[", 
          RowBox[{"m", ",", "n"}], "]"}]}], ",", 
        RowBox[{"kM", "\[Rule]", 
         RowBox[{"Max", "[", 
          RowBox[{"m", ",", "n"}], "]"}]}]}], "}"}]}], ",", "0"}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.796134210962214*^9, 3.7961342195728493`*^9}, {
  3.796136999651174*^9, 3.796137025536384*^9}},
 CellLabel->"In[81]:=",ExpressionUUID->"236a204f-bd9c-4723-9014-28737a9ccfdb"]
}, Closed]],

