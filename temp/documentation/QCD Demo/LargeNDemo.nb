          RowBox[{"(", 
           RowBox[{"m", "+", "n"}], ")"}], " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"m", "-", "n"}], ")"}], " ", "\[Pi]"}], "]"}]}], "+", 
         RowBox[{"2", " ", "m", " ", "n", " ", 
          RowBox[{"CosIntegral", "[", 
           RowBox[{"n", " ", "\[Pi]"}], "]"}]}], "-", 
         RowBox[{
