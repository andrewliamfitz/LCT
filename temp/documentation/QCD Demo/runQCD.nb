Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"monoExpand", "[", 
    RowBox[{"op_", "?", "ListQ"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"monoExpand", "[", "op", "]"}], "=", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"op", "[", 
        RowBox[{"[", "4", "]"}], "]"}], "\[NotEqual]", "1"}], ",", 
      RowBox[{"Block", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"l", "=", 
           RowBox[{"op", "[", 
            RowBox[{"[", "3", "]"}], "]"}]}], ",", 
          RowBox[{"dim1", "=", 
           RowBox[{"op", "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "5"}], "]"}], "]"}]}], ",", 
          RowBox[{"dim2", "=", 
           RowBox[{"op", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "5"}], "]"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "coeff", ",", 
          RowBox[{"n1", "=", 
           RowBox[{"op", "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "4"}], "]"}], "]"}]}], ",", 
          RowBox[{"n2", "=", 
           RowBox[{"op", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "4"}], "]"}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"op1", "=", 
           RowBox[{"monoExpand", "[", 
            RowBox[{"op", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ",", 
          RowBox[{"op2", "=", 
           RowBox[{"monoExpand", "[", 
            RowBox[{"op", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "dMonosList1", ",", "dMonosList2"}], 
         "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"dMonosList1", "=", 
          RowBox[{"dMonosList", "[", " ", 
           RowBox[{
            RowBox[{"monoExpand", "[", 
             RowBox[{"op", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", "l"}], " ", "]"}]}],
          ";", "\[IndentingNewLine]", 
         RowBox[{"dMonosList2", "=", 
          RowBox[{"dMonosList", "[", " ", 
           RowBox[{
            RowBox[{"monoExpand", "[", 
             RowBox[{"op", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "]"}], ",", "l"}], " ", "]"}]}],
          ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"DeleteCases", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", 
                  RowBox[{";;", ",", "1"}], "]"}], "]"}], "//", "Total"}], 
               ",", 
               RowBox[{"#", "[", 
                RowBox[{"[", 
                 RowBox[{"1", ",", "2"}], "]"}], "]"}]}], "}"}], "&"}], "/@", 
            
            RowBox[{"GatherBy", "[", 
             RowBox[{
              RowBox[{"Flatten", "[", 
               RowBox[{
                RowBox[{"Table", "[", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"coeff", "=", 
                    RowBox[{
                    RowBox[{"PrimCoeffs", "[", 
                    RowBox[{"dim1", ",", "dim2", ",", "l", ",", "k"}], "]"}], 
                    "*", "\[IndentingNewLine]", 
                    RowBox[{"mono1", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "*", 
                    RowBox[{"mono2", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}]}], ";", 
                   "\[IndentingNewLine]", 
                   RowBox[{"{", 
                    RowBox[{"coeff", ",", 
                    RowBox[{"Sort", "[", 
                    RowBox[{"Join", "[", 
                    RowBox[{
                    RowBox[{"mono1", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], ",", 
                    RowBox[{"mono2", "[", 
                    RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "]"}]}], "}"}]}],
                   ",", "\[IndentingNewLine]", 
                  RowBox[{"{", 
                   RowBox[{"k", ",", "0", ",", "l"}], "}"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"{", 
                   RowBox[{"mono1", ",", 
                    RowBox[{"dMonosList1", "[", 
                    RowBox[{"[", 
                    RowBox[{"k", "+", "1"}], "]"}], "]"}]}], "}"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"{", 
                   RowBox[{"mono2", ",", 
                    RowBox[{"dMonosList2", "[", 
                    RowBox[{"[", 
                    RowBox[{"l", "-", "k", "+", "1"}], "]"}], "]"}]}], 
                   "}"}]}], "\[IndentingNewLine]", "]"}], ",", "2"}], "]"}], 
