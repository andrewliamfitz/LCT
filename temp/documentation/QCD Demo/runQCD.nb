329b3b919543"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "stateSet", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"stateSet", "[", "dim_", "]"}], ":=", 
  RowBox[{
   RowBox[{"stateSet", "[", "dim", "]"}], "=", 
   RowBox[{"DeleteDuplicatesBy", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Select", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Block", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", " ", 
          RowBox[{"set", "=", 
           RowBox[{"{", "}"}]}], " ", "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"dim", "\[GreaterEqual]", "1"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"(*", " ", 
             RowBox[{
             "There", " ", "is", " ", "always", " ", "exactly", " ", "one", 
              " ", "2", " ", "particle", " ", 
              RowBox[{"state", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"AppendTo", "[", 
              RowBox[{"set", ",", " ", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"-", "1"}], ",", "1", ",", 
                 RowBox[{"dim", "-", "1"}], ",", "1", ",", "dim"}], "}"}]}], 
              "]"}], ";", "\[IndentingNewLine]", 
             RowBox[{"Do", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"AppendTo", "[", 
                RowBox[{"set", ",", 
                 RowBox[{"{", 
                  RowBox[{"state1", ",", "state2", ",", 
                   RowBox[{"dim", "-", "dim1", "-", "dim2"}], ",", 
                   RowBox[{
                    RowBox[{"state1", "[", 
                    RowBox[{"[", "4", "]"}], "]"}], "+", 
                    RowBox[{"state2", "[", 
                    RowBox[{"[", "4", "]"}], "]"}]}], ",", "dim"}], "}"}]}], 
                "]"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"dim1", ",", 
                 RowBox[{"dim", "-", "1"}], ",", "1", ",", 
                 RowBox[{"-", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"dim2", ",", "1", ",", 
                 RowBox[{"Min", "[", 
                  RowBox[{
                   RowBox[{"dim", "-", "dim1"}], ",", "dim1"}], "]"}], ",", 
                 "1"}], "}"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"state1", ",", 
                 RowBox[{"stateSet", "[", "dim1", "]"}]}], "}"}], ",", 
               "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"state2", ",", 
                 RowBox[{"stateSet", "[", "dim2", "]"}]}], "}"}]}], "]"}]}]}],
            "  ", "]"}], ";", "\[IndentingNewLine]", "set"}]}], 
        "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Some", " ", "double", " ", "trace", " ", "operators", " ", 
          RowBox[{"don", "'"}], "t", " ", "exist"}], ",", " ", 
         RowBox[{
         "and", " ", "their", " ", "monomial", " ", "expansion", " ", "are", 
          " ", 
          RowBox[{"zero", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"monoExpand", "[", "#", "]"}], "\[NotEqual]", 
         RowBox[{"{", "}"}]}], "&"}]}], "]"}], ",", "monoExpand"}], 
    "]"}]}]}]}], "Input",
 CellChangeTimes->{{3.783459780552373*^9, 3.783459798631007*^9}, {
