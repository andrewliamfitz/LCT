Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "The", " ", "coefficients", " ", "in", " ", "the", " ", "double", " ", 
    "trace", " ", "construction"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"PrimCoeffs", "[", 
     RowBox[{
     "\[CapitalDelta]A_", ",", "\[CapitalDelta]B_", ",", "l_", ",", "k_"}], 
     "]"}], "=", 
    FractionBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "k"], 
      RowBox[{"Gamma", "[", 
       RowBox[{
        RowBox[{"2", "\[CapitalDelta]A"}], "+", "l"}], "]"}], 
      RowBox[{"Gamma", "[", 
       RowBox[{
        RowBox[{"2", "\[CapitalDelta]B"}], "+", "l"}], "]"}]}], 
     RowBox[{
      RowBox[{"k", "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"l", "-", "k"}], ")"}], "!"}], 
      RowBox[{"Gamma", "[", 
       RowBox[{
        RowBox[{"2", "\[CapitalDelta]A"}], "+", "k"}], "]"}], 
      RowBox[{"Gamma", "[", 
       RowBox[{
        RowBox[{"2", "\[CapitalDelta]B"}], "+", "l", "-", "k"}], "]"}]}]]}], 
   ";"}]}]], "Input",
 CellChangeTimes->{{3.796251141191052*^9, 3.7962511610816107`*^9}},
 CellLabel->
  "In[179]:=",ExpressionUUID->"daa1e82c-13bf-470e-ab99-2f6bd1f3ddae"],

Cell[TextData[{
 StyleBox["stateSet[dim]",
  FontWeight->"Bold"],
 " computes the basis states at dimension ",
 StyleBox["dim",
  FontWeight->"Bold"],
 " from taking the double-trace construction of lower ",
 StyleBox["dim",
  FontWeight->"Bold"],
 ". ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[ScriptCapitalO]", "1"], 
     RowBox[{
      OverscriptBox[
       SuperscriptBox["\[PartialD]", "\[ScriptL]"], "\[TwoWayRule]"], 
      SubscriptBox["\[ScriptCapitalO]", "2"]}]}], "\[Congruent]", 
    RowBox[{
     SubscriptBox["\[Sum]", "k"], 
     RowBox[{
      RowBox[{"PrimCoeffs", "[", 
       RowBox[{
       "\[CapitalDelta]1", ",", "\[CapitalDelta]2", ",", "\[ScriptL]", ",", 
        "k"}], "]"}], " ", 
      RowBox[{
       SuperscriptBox["\[PartialD]", "k"], " ", 
       SubscriptBox["\[ScriptCapitalO]", "1"]}], 
      RowBox[{
       SuperscriptBox["\[PartialD]", 
        RowBox[{"\[ScriptL]", "-", "k"}]], 
       SubscriptBox["\[ScriptCapitalO]", "2"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"12ee1f50-e3b5-4eee-ba0a-d4841fb2d2b6"],
 " \nThe output of ",
 StyleBox["stateSet[dim]",
  FontWeight->"Bold"],
 " is a list of operators written as\n\t{",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "1"], TraditionalForm]],ExpressionUUID->
  "53035d6f-420c-46f8-a635-e7b6b3d677b8"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "2"], TraditionalForm]],ExpressionUUID->
  "365ef1b4-bbe4-4943-bd28-c41334bdfa70"],
 ", \[ScriptL], n, \[CapitalDelta] }\nwhere ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "1"], TraditionalForm]],ExpressionUUID->
  "4d060241-2780-4b53-b498-e38997bfc2dc"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "2"], TraditionalForm]],ExpressionUUID->
  "af54d414-0b11-41d4-ac5e-4008c3a79dcc"],
 " are the two low dim operators that the operator is made of,\n\[ScriptL] is \
the total number of derivatives acting on ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "1"], TraditionalForm]],ExpressionUUID->
  "c28c34cf-0193-4f06-bb93-1355740c584f"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalO]", "2"], TraditionalForm]],ExpressionUUID->
  "0bf2d347-77af-4c29-9d89-411b305bc7fb"],
 ",\nn is half of the total number of particles,\n\[CapitalDelta] is the \
total dimension, ",
 StyleBox["dim",
  FontWeight->"Bold"],
 ".\nThe beginning seed are operators \[Psi] and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Psi]", "\[Dagger]"], TraditionalForm]],ExpressionUUID->
  "80c173e3-a62c-4a0c-bb90-c29dbfbd4711"],
 ", represented by 1 and (-1), respectively."
}], "Text",
 CellChangeTimes->{{3.796251236684901*^9, 3.796251305743861*^9}, {
  3.796251512743268*^9, 3.796251535737093*^9}, {3.7962516452716093`*^9, 
  3.7962518396788683`*^9}, {3.796251884013843*^9, 3.796251933042637*^9}, {
  3.7962527333870783`*^9, 3.796252854312742*^9}, {3.796252935168092*^9, 
